<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="650.0" prefWidth="786.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MatchListController">
   <children>
      <TableView fx:id="matchTable" layoutX="40.0" layoutY="98.0" prefHeight="509.0" prefWidth="460.0">
        <columns>
          <TableColumn fx:id="date" prefWidth="100.0" text="Date" />
          <TableColumn fx:id="opponent" minWidth="5.0" prefWidth="100.0" text="Opponent" />
            <TableColumn fx:id="place" minWidth="5.0" prefWidth="100.0" text="Place" />
            <TableColumn fx:id="kind" minWidth="5.0" prefWidth="100.0" text="Kind of Match" />
        </columns>
      </TableView>
      <Text layoutX="265.0" layoutY="71.0" onDragDetected="#reload" strokeType="OUTSIDE" strokeWidth="0.0" text="MATCHES">
         <font>
            <Font size="59.0" />
         </font>
      </Text>
      <Button fx:id="editButton" layoutX="587.0" layoutY="118.0" mnemonicParsing="false" onAction="#actionMatch" prefHeight="46.0" prefWidth="99.0" text="edit" />
      <Button fx:id="addMatch" layoutX="587.0" layoutY="194.0" mnemonicParsing="false" onAction="#actionMatch" prefHeight="53.0" prefWidth="99.0" text="Add Match" />
      <Button fx:id="deleteMatch" layoutX="587.0" layoutY="278.0" mnemonicParsing="false" onAction="#deleteMatch" prefHeight="59.0" prefWidth="99.0" text="Delete Match" />
      <Button fx:id="reload" layoutX="587.0" layoutY="561.0" mnemonicParsing="false" onAction="#reload" text="REload" />
      <Button fx:id="write" layoutX="659.0" layoutY="562.0" mnemonicParsing="false" onAction="#write" text="Write" />
   </children>
</AnchorPane>
